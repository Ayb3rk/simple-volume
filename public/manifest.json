{
  "manifest_version": 3,
  "name": "Simple Volume",
  "version": "2.0",
  "description": "Control volume per tab.",
  "permissions": ["tabs", "scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "icons/icon128.png"
    },
    "default_title": "Simple Volume"
  },
  "icons": {
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
